<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>vmruntime-demo</application>
  <version>fractal</version>
  <threadsafe>true</threadsafe>
  <module>default</module>

  <vm>true</vm>
  <manual-scaling>
    <instances>2</instances>
  </manual-scaling>

  <precompilation-enabled>false</precompilation-enabled>
    <vm-settings>
        <setting name="allow_ssh" value="true"/>
    </vm-settings>
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    <property name="LUDOVIC" value="is @ JavaOne 2014. Cool..." />
  </system-properties>
    
  <env-variables>
    <env-var name="CDBG_ENABLE" value="1" />
    <env-var name="DBG_PORT" value="5005" />
  </env-variables>
</appengine-web-app>
